<p id="notice"><%= notice %></p>

<h1>Hanghoas</h1>

<table>
  <thead>
    <tr>
      <th>Tensp</th>
      <th>Mota</th>
      <th>Anh</th>
      <th>Soluong</th>
      <th>Dongia</th>
      <th>Iddanhmuc</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @hanghoas.each do |hanghoa| %>
      <tr>
        <td><%= hanghoa.tensp %></td>
        <td><%= hanghoa.mota %></td>
        <td><%= hanghoa.anh %></td>
        <td><%= hanghoa.soluong %></td>
        <td><%= hanghoa.dongia %></td>
        <td><%= hanghoa.iddanhmuc %></td>
        <td><%= link_to 'Show', hanghoa %></td>
        <td><%= link_to 'Edit', edit_hanghoa_path(hanghoa) %></td>
        <td><%= link_to 'Destroy', hanghoa, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Hanghoa', new_hanghoa_path %>
